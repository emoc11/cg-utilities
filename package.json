{
	"name": "cg-utilities",
	"version": "1.0.0",
	"description": "Utilitaires TypeScript modulaires avec unbuild",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./easingUtils": {
			"import": "./dist/easingUtils.mjs",
			"require": "./dist/easingUtils.cjs",
			"types": "./dist/easingUtils.d.ts"
		},
		"./threeUtils": {
			"import": "./dist/threeUtils.mjs",
			"require": "./dist/threeUtils.cjs",
			"types": "./dist/threeUtils.d.ts"
		}
	},
	"scripts": {
		"prefix-types": "node node/rename-types.js",
		"generate-exports": "node node/generate-exports.js",
		"dev": "unbuild --stub",
		"build": "unbuild",
		"prepare": "yarn generate-exports && yarn build && yarn prefix-types"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"utils",
		"typescript",
		"easing",
		"modular"
	],
	"author": "Come Gaillard",
	"license": "MIT",
	"devDependencies": {
		"@types/three": "^0.175.0",
		"gsap": "^3.12.7",
		"three": "^0.175.0",
		"typescript": "^5.8.3",
		"unbuild": "^3.5.0"
	},
	"peerDependencies": {
		"gsap": "^3.12.7",
		"three": "^0.175.0"
	},
	"packageManager": "yarn@1.22.0"
}
